  <appSettings>
    <add key="Page Version"  value="$LastChangedRevision: 26605 $"/>
    <add key="ThisAppName"  value="GTM"/>
    <add key="sqlGetGMI_STATUS_LOV"  value="select distinct valid_value from ctl_rules_validation_single where source_column='STATUS' order by valid_value"/>
    <add key="sqlGetGMI_ENV_LOV"     value="select distinct valid_value from ctl_rules_validation_single where source_column='ENVIRONMENT' and valid_value not like '%(%)%' order by valid_value"/>
    <add key="sqlGetGMI_FEED_LOV"    value="select filter_value from (select distinct ctl_process_filter.filter_value, ctl_process_flow.feed_priority from ctl_process_filter inner join ctl_process_flow on ctl_process_filter.feed_table=ctl_process_flow.feed_table order by ctl_process_flow.feed_priority ASC)"/>
    <add key="GMI_TRANS_select"      value="SELECT gtrans.gmi_initiating_process as feed,
                                                gtrans.hostname,
                                                gtrans.gmi_environment,
                                                gtrans.gmi_status, 
                                                gtrans.gmi_infrastructure as INF, 
                                                gtrans.gmi_capability as CAP, 
                                                gtrans.gmi_system_group as LSG,
                                                CTLR.LAST_PROCESS_TIME as LAST_TRANSFORM, 
                                                tb.blocking /*SELECT_MODIFIER*/ "/>
    <add key="GMI_TRANS_from"        value="
                                            FROM gmi_trans gtrans, 
                                                ctl_log_process_records ctlr, 
                                                ctl_process_filter pf, 
                                                gmi_trans_block tb /*FROM_MODIFIER*/ "/>
    <add key="GMI_TRANS_joinview"    value="   , (select max(ct.src_record_id) as recid, 
                                                      GT.GMI_INITIATING_PROCESS, 
                                                      GT.HOSTNAME, 
                                                      GT.GMI_ENVIRONMENT, 
                                                      gt.process_id /*INNER_SELECT_MODIFIER*/
                                                  from ctl_log_process_records ct, 
                                                      gmi_trans gt /*INNER_FROM_MODIFIER*/
                                                  where ct.src_record_id = GT.SRC_RECORD_ID 
                                                      and ct.process_id = gt.process_id  /*INNER_WHERE_MODIFIER*/
                                                  group by  GT.GMI_INITIATING_PROCESS, GT.HOSTNAME, GT.GMI_ENVIRONMENT, gt.process_id /*INNER_GROUPBY_MODIFIER*/
                                                  ) var1  "/>
    <add key="GMI_TRANS_where"    value=" 
                                              where var1.recid = gtrans.src_record_id
                                              and var1.recid = ctlr.src_record_id
                                              and var1.process_id = ctlr.process_id
                                              and ctlr.process_id = gtrans.process_id
                                              and gtrans.process_id = pf.process_id
                                              and gtrans.hostname = tb.hostname (+)
                                              and gtrans.gmi_initiating_process = tb.gmi_initiating_process (+)
                                              /*WHERE_MODIFIER*/ "/>
    <add key="GMI_TRANS_orderby"     value=" 
                                              order by gtrans.hostname, gtrans.gmi_initiating_process "/>
    
    <add key="GMI_REF_select"     value="select HOSTNAME,GMI_LOCATION,GMI_SOURCE, gmi_initiating_process,GMI_INFRASTRUCTURE as INF,gmi_capability as CAP,gmi_system_group as LSG,gmi_status,last_updated from GMI_REF where HOSTNAME="/>
         
